using namespace std;
#include<algorithm>
int partition(int arr[],int start,int end){
    int pirot=arr[end];
    int j=start-1;
    for(int i=start;i<end;i++){
        if(arr[i]<pirot){
            j++;
            swap(arr[j],arr[i]);
        }
        
    }
    swap(arr[j+1],arr[end]);
    return j+1;
}
void QS(int arr[],int start,int end){
    if(start<end){
        int pirotIndex=partition(arr,start,end);
        QS(arr,start,pirotIndex-1);
        QS(arr,pirotIndex+1,end);
    }
}
int main(){
    int arr[]={2,5,9,3,6,10};
    int n=sizeof (arr)/sizeof (arr[0]);
    QS(arr,0,n-1);
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
}
